{
    "name": "jerome-rdlv/sywo",
    "description": "Symfony in WordPress integration",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rdlv\\WordPress\\Sywo\\": "src"
        }
    },
    "authors": [
        {
            "name": "Jérôme Mulsant",
            "email": "jerome@rue-de-la-vieille.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/console": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/http-kernel": "^5.4"
    },
    "suggest": {
        "symfony/framework-bundle": "For tight integration between symfony components",
        "symfony/translation": "For translation update command",
        "symfony/web-profiler-bundle": "For easy debug and profiling"
    }
}
